<div id="<%= dom_id movie %>">
  <p>
    <strong>Title:</strong>
    <%= movie.title %>
  </p>

  <p>
    <strong>Audience score:</strong>
    <%= movie.audience_score %>
  </p>

  <p>
    <strong>Rt score:</strong>
    <%= movie.rt_score %>
  </p>

  <p>
    <strong>Profitability:</strong>
    <%= movie.profitability %>
  </p>

  <p>
    <strong>Gross:</strong>
    <%= movie.gross %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= movie.year %>
  </p>
</div>
